#!ipxe

echo Arch iPXE bootloader

# Load kernel with proper parameters
kernel http://nginx.ryuugu.dev/arch_netboot/vmlinuz archisobasedir=arch archiso_http_srv=http://nginx.ryuugu.dev/arch_netboot/ ip=dhcp cow_spacesize=512M initrd=initrd

# Load initial ramdisk
initrd http://nginx.ryuugu.dev/arch_netboot/initrd.img

# Boot the system
boot
